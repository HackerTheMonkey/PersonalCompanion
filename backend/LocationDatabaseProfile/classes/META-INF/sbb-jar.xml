<?xml version="1.0"?>
<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.1//EN" 
    "http://java.sun.com/dtd/slee-sbb-jar_1_1.dtd">
<sbb-jar>
  <sbb id="MySbb">

    <sbb-name>MySbb</sbb-name>
    <sbb-vendor>OpenCloud</sbb-vendor>
    <sbb-version>1.0</sbb-version>

    <profile-spec-ref>
      <profile-spec-name>LocationDatabaseProfileSpec</profile-spec-name>
      <profile-spec-vendor>OpenCloud</profile-spec-vendor>
      <profile-spec-version>1.0</profile-spec-version>
      <profile-spec-alias>LocationDatabaseProfileSpec</profile-spec-alias>
    </profile-spec-ref>

    <sbb-classes>
      <sbb-abstract-class>
        <sbb-abstract-class-name>com.opencloud.slee.services.myservice.MySbb</sbb-abstract-class-name>
        <get-profile-cmp-method>
          <profile-spec-alias-ref>LocationDatabaseProfileSpec</profile-spec-alias-ref>
          <get-profile-cmp-method-name>getLocationDatabaseProfileSpec</get-profile-cmp-method-name>
        </get-profile-cmp-method>
      </sbb-abstract-class>
    </sbb-classes>

    <event event-direction="Receive" initial-event="True">
      <event-name>ServiceStartedEvent</event-name>
      <event-type-ref>
        <event-type-name>javax.slee.serviceactivity.ServiceStartedEvent</event-type-name>
        <event-type-vendor>javax.slee</event-type-vendor>
        <event-type-version>1.0</event-type-version>
      </event-type-ref>
      <initial-event-select variable="ActivityContext" />
    </event>

  </sbb>
</sbb-jar>
