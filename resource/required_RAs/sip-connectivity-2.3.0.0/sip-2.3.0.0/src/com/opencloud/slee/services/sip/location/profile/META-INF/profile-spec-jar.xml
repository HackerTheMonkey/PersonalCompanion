<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE profile-spec-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Profile Specification 1.1//EN" "http://java.sun.com/dtd/slee-profile-spec-jar_1_1.dtd">

<profile-spec-jar>

  <profile-spec id="registrations" profile-read-only="False">
    <description>Profile for storing SIP registrations</description>
    <profile-spec-name>${location.profile.name}</profile-spec-name>
    <profile-spec-vendor>${location.profile.vendor}</profile-spec-vendor>
    <profile-spec-version>${location.profile.version}</profile-spec-version>

    <profile-classes>
      <profile-cmp-interface>
        <profile-cmp-interface-name>
          com.opencloud.slee.services.sip.location.profile.RegistrationProfileCMP
        </profile-cmp-interface-name>
        <cmp-field unique="True">
          <cmp-field-name>addressOfRecord</cmp-field-name>
        </cmp-field>
      </profile-cmp-interface>
      <profile-local-interface>
        <profile-local-interface-name>
          com.opencloud.slee.services.sip.location.profile.RegistrationProfileLocal
        </profile-local-interface-name>
      </profile-local-interface>
      <profile-management-interface>
        <profile-management-interface-name>
          com.opencloud.slee.services.sip.location.profile.RegistrationProfileManagement
        </profile-management-interface-name>
      </profile-management-interface>
      <profile-abstract-class>
        <profile-abstract-class-name>
          com.opencloud.slee.services.sip.location.profile.RegistrationProfile
        </profile-abstract-class-name>
      </profile-abstract-class>
    </profile-classes>
  </profile-spec>

</profile-spec-jar>

