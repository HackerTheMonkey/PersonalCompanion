<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE service-xml PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Service 1.1//EN" "http://java.sun.com/dtd/slee-service_1_1.dtd">

<service-xml>
  <service>
    <service-name>${location.profile.service.name}</service-name>
    <service-vendor>${location.profile.service.vendor}</service-vendor>
    <service-version>${location.profile.service.version}</service-version>
    <root-sbb>
      <sbb-name>${location.profile.sbb.name}</sbb-name>
      <sbb-vendor>${location.profile.sbb.vendor}</sbb-vendor>
      <sbb-version>${location.profile.sbb.version}</sbb-version>
    </root-sbb>
    <default-priority>0</default-priority>
  </service>
</service-xml>
